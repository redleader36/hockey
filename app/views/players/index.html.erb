<% title "Players" %>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Number</th>
  </tr>
  <% for player in @players %>
    <tr>
      <td><%= player.first_name %></td>
      <td><%= player.last_name %></td>
      <td><%= player.number %></td>
      <td><%= link_to "Show", player %></td>
    </tr>
  <% end %>
</table>

